<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myissuesinterface.IssuesController">
   <stylesheets>
      <URL value="@../../../../../../../../../../../../myissues/myissues_App/src/myissues_app/myissuesStyle.css" />
